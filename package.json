{
  "name": "phantom-backend-pirates",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node src/app.js",
    "test": "nyc mocha --require @babel/register",
    "lint": "eslint . --fix --ext .js",
    "migrate": "sequelize db:migrate",
    "undomigrate": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "undoseed": "sequelize db:seed:undo:all",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add -A ."
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/Phantom-Backend-Pirates.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/Phantom-Backend-Pirates/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/Phantom-Backend-Pirates#readme",
  "dependencies": {
    "@babel/node": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.17.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "http-errors": "^2.0.0",
    "husky": "^7.0.4",
    "i18next": "^15.1.1",
    "i18next-express-middleware": "^1.8.0",
    "i18next-http-middleware": "^3.2.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.16.2",
    "smws": "^1.0.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.4",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "coverage": "^0.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "sinon": "^13.0.1"
  }
}
